Host *
  SetEnv TERM="xterm-256color"
  AddKeysToAgent yes
  ControlMaster auto
  ControlPath ~/.ssh/control/%r@%h:%p
  ControlPersist 10m
  ServerAliveCountMax 3
  ServerAliveInterval 30
  StrictHostKeyChecking accept-new

  
